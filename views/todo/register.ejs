<script type="text/x-handlebars" data-template-name="register">
  <div class="container">
    <form class="form-signin" role="form" action="user/login" method="post">
      <h2 class="form-signin-heading">Register</h2>
      {{input value=email type="text" class="form-control" placeholder="Email address" }}
      {{input value=password type="password" class="form-control" placeholder="Password" }}
      {{input class="btn btn-lg btn-success btn-block" type="submit" value="Register"}}
    </form>
  </div>
</script>
